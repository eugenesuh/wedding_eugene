<div class="page-header">
  <h1>
    Slikes
    <a href="/slikes/new" class="btn btn-lg btn-success">New Slike</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Song</th>
        <th>Actions</th>
      </tr>

      <% @slikes.each do |slike| %>
      <tr>
        <td><%= slike.user_id %></td>
        <td><%= slike.song_id %></td>
        <td>
          <a href="/slikes/<%= slike.id %>" class="btn btn-primary">Show</a>
          <a href="/slikes/<%= slike.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_slike/<%= slike.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
